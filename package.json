{
  "name": "react-nos-kit",
  "version": "0.1.0",
  "description": "React Node Started Kit",
  "scripts": {
    "start-message": "babel-node tools/startMessage.js",
    "prestart": "npm-run-all --parallel start-message remove-dist",
    "start": "npm-run-all --parallel test:watch open:src lint:watch start-server",
    "start-server": "nodemon --exec babel-node ./server/server.js",
    "open:src": "babel-node tools/srcServer.js",
    "lint": "esw webpack.config.* src tools",
    "lint:watch": "npm run lint -- --watch",
    "clean-dist": "npm run remove-dist && mkdir dist",
    "remove-dist": "rimraf ./dist",
    "build": "npm run clean-dist && npm run build:html && npm run build:client && npm run build:server",
    "build:html": "node tools/buildHtml.js",
    "build:server": "babel -d ./dist ./server -s",
    "build:client": "node tools/buildClient.js",
    "heroku-postbuild": "npm run build",
    "test": "mocha tools/testSetup.js \"src/**/*.spec.js\" --reporter progress",
    "test:cover": "babel-node node_modules/isparta/bin/isparta cover --root src --report html node_modules/mocha/bin/_mocha -- --require ./tools/testSetup.js \"src/**/*.spec.js\" --reporter progress",
    "test:cover:travis": "babel-node node_modules/isparta/bin/isparta cover --root src --report lcovonly _mocha -- --require ./tools/testSetup.js \"src/**/*.spec.js\" && cat ./coverage/lcov.info | node_modules/coveralls/bin/coveralls.js",
    "test:watch": "npm run test -- --watch"
  },

  "engines": {
    "node": "6.2.1",
    "npm": "3.9.3"
  },
  "author": "Oleg Umarov",
  "license": "MIT",
  "dependencies": {
    "babel-cli": "6.8.0",
    "body-parser": "1.15.2",
    "express": "4.14.0",
    "extract-text-webpack-plugin": "1.0.1",
    "cheerio": "0.20.0",
    "object-assign": "4.1.0",
    "react": "15.0.2",
    "react-dom": "15.0.2",
    "react-redux": "4.4.5",
    "react-router": "2.4.0",
    "rimraf": "2.5.2",
    "redux": "3.5.2",
    "webpack": "1.13.0"
  },
  "devDependencies": {
    "babel-core": "6.8.0",
    "babel-loader": "6.2.4",
    "babel-plugin-react-display-name": "2.0.0",
    "babel-preset-es2015": "6.6.0",
    "babel-preset-react": "6.5.0",
    "babel-preset-react-hmre": "1.1.1",
    "babel-preset-stage-1": "6.5.0",
    "babel-register": "6.8.0",
    "browser-sync": "2.12.5",
    "chai": "3.5.0",
    "chalk": "1.1.3",
    "connect-history-api-fallback": "1.2.0",
    "coveralls": "2.11.9",
    "cross-env": "1.0.7",
    "css-loader": "0.23.1",
    "enzyme": "2.2.0",
    "eslint": "2.9.0",
    "eslint-plugin-import": "1.6.1",
    "eslint-plugin-jsx-a11y": "1.0.4",
    "eslint-plugin-react": "5.0.1",
    "eslint-watch": "2.1.11",
    "file-loader": "0.8.5",
    "isparta": "4.0.0",
    "mocha": "2.4.5",
    "node-sass": "3.7.0",
    "nodemon": "1.9.2",
    "npm-run-all": "1.8.0",
    "prompt": "1.0.0",
    "react-addons-test-utils": "15.0.2",
    "redux-immutable-state-invariant": "1.2.3",
    "replace": "0.3.0",
    "sass-loader": "3.2.0",
    "sinon": "1.17.4",
    "sinon-chai": "2.8.0",
    "style-loader": "0.13.1",
    "webpack-dev-middleware": "1.6.1",
    "webpack-hot-middleware": "2.10.0"
  },
  "keywords": [],
  "repository": {
    "type": "git",
    "url": ""
  }
}
